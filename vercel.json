{
  "version": 2,
  "builds": [
    {
      "src": "index.py",
      "use": "@vercel/python",
      "config": {
        "includeFiles": "public/**"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.py"
    }
  ],
  "crons": [
    {
      "path": "/api/v1/cron/trigger_draws",
      "schedule": "0 21 30 8 *"
    }
  ]
}
